{
  "Jwt": {
    "Key": "your-secret-key-here-minimum-16-characters",
    "Issuer": "https://adminflow.ru",
    "Audience": "https://adminflow.ru"
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": [
      "https://adminflow.ru",
      "https://www.adminflow.ru",
      "http://adminflow.ru",
      "http://www.adminflow.ru"
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5002"
      },
      "Https": {
        "Url": "https://localhost:5002",
        "Certificate": {
          "Path": "/path/to/your/certificate.pfx",
          "Password": "your-certificate-password"
        }
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/path/to/your/certificate.pfx",
        "Password": "your-certificate-password"
      }
    }
  }
} 